 {{#each beers}}
        <div>
            {{#if this.image_url}}
                <img src="{{ this.image_url }}" alt="{{ this.tagline }} />
            {{/if}}
            <div>
                <h3>
                    <a href="/beers/{{this.id}}"></a>
                    {{this.name}}
                    </a>
                </h3>
                <h5>{{this.tagline}}</h5>
                <p>{{this.description}}</p>
            </div>
        </div>
        
    {{/each}}

    <div>
        <a href="/random-beer">Choose another random beer</a>
    </div>
<br>